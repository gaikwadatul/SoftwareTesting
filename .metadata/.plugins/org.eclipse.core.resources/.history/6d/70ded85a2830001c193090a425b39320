package String_class;

class demo1223{
	private demo1223() {
		System.out.println("I am xero-param");
	}
	
	static demo1223 d1 = new demo1223();
	String name = "Pune"
			
	static demo1223 getInstance() {
		return d1;
	}	
	void print() {
		System.out.println("I am print() of singleton");
	}
}
