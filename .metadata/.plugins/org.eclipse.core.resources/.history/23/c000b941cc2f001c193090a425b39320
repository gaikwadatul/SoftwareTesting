package String_class;

final class TestImmutableClass{
	final String name;
	final int salary;
	TestImmutableClass(String s1, int num1){
		name= s1;
		salary=num1;
	}
	
	public String getName() {
		return name;
		
	}
}

public class Immutable_class {

	public static void main(String[] args) {
		System.out.println("Hi");

	}

}
